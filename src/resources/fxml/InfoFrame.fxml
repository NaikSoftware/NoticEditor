<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <Label text="TreeNote Project (version 1.1)">
            <font>
                <Font size="34.0"/>
            </font>
        </Label>
        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/resources/images/tree_note.png"/>
            </image>
        </ImageView>
        <TextArea editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  prefHeight="200.0" prefWidth="200.0" wrapText="true" text="%prog_info">
            <font>
                <Font size="15.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </TextArea>
        <Label text="%copyright" wrapText="true">
            <font>
                <Font size="15.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </Label>
    </children>
</VBox>
